{
  "name": "frontend",
  "version": "5.55.2",
  "author": "Weplay FE team",
  "private": true,
  "license": "ISC",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "sideEffects": [
    "*.(scss|css)"
  ],
  "scripts": {
    "cdn-qa-gz": "cross-env COMPRESSION=gz CDN_PATH=https://cdn-qa.weplay.tv/ yarn run production",
    "cdn-qa-br": "cross-env COMPRESSION=br CDN_PATH=https://cdn-qa.weplay.tv/ yarn run production",
    "analytics": "cross-env ANALYTICS=1 --max-old-space-size=3000 node_modules/webpack-dev-server/bin/webpack-dev-server.js -r esm",
    "b2b-build": "cd packages/b2b-service && yarn build",
    "b2b-start": "cd packages/b2b-service && yarn start",
    "ssr-platform-service": "cd packages/ssr-platform-service && yarn build",
    "build": "webpack -r esm",
    "start": "cross-env IS_SOURCEMAPS_ENABLED=1 node --max-old-space-size=3000 node_modules/webpack-dev-server/bin/webpack-dev-server.js -r esm",
    "events": "cross-env TARGET_PACKAGE=events webpack-dev-server -r esm",
    "tournaments": "cross-env TARGET_PACKAGE=tournaments webpack-dev-server -r esm",
    "media": "cross-env TARGET_PACKAGE=media webpack-dev-server -r esm",
    "production": "cross-env NODE_ENV=production yarn run build",
    "prod": "yarn run production",
    "start-prod": "cross-env NODE_ENV=production yarn run start",
    "prod-analyze": "cross-env NODE_ENV=production ANALYZE=1 yarn run start",
    "dev-analyze": "cross-env ANALYZE=1 yarn run start",
    "debug-webpack": "nodemon -r esm --watch webpack.config.js ./node_modules/.bin/webpack-dev-server",
    "storybook": "node -r esm  node_modules/.bin/start-storybook -p 3000",
    "build-storybook": "node -r esm  node_modules/.bin/build-storybook",
    "publish-dev": "lerna -v && lerna publish --conventional-commits --yes --changelog-preset angular-bitbucket --canary --no-git-reset",
    "dota-build": "lerna run build --scope=dota2-game-coordinator-service --stream",
    "eslint-all": "eslint --fix --config=./eslintrc.js "
  },
